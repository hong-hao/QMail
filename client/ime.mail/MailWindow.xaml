<q:QWindow x:Class="ime.mail.MailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:q="clr-namespace:ime.controls.QWindow;assembly=ime.controls"
        Style="{StaticResource QWindow}" ShowInQSTaskBar="True" ShowInTaskbar="False" WindowStartupLocation="CenterScreen"
        Title="QS Mail" Height="600" Width="1000" xmlns:c="clr-namespace:ime.controls;assembly=ime.controls" Icon="/ime.mail;component/Icons/mail.png" xmlns:control="clr-namespace:ime.mail.controls" xmlns:v="clr-namespace:ime.mail.views">
    <Grid Name="root">
        <v:MailView x:Name="mailView"></v:MailView>
        <Grid Name="customerManager"></Grid>
        <c:FlexAppContainer x:Name="calendarApp" Url="/pages/calendar/CalendarApp.swf" Visibility="Collapsed"></c:FlexAppContainer>
    </Grid>
</q:QWindow>
